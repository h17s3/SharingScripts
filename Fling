cdb1 = false
--//.MouseButton1Click:Connect(function()
TFM.MouseButton1Click:Connect(function()
	if not workspace:FindFirstChild("non") then
		if cdb1 == false then
			cdb1 = true
			if game.Players.LocalPlayer.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
				_G.TorsoFling = true
			else
				_G.TorsoFling = false
				_G.Fling = true
			end
			_G.ShowReal = true
			loadstring(game:HttpGet("https://raw.githubusercontent.com/Tescalus/Pendulum-Hubs-Source/main/NetlessReanimV2(ThanksProductionTakeOne%20%233330%20for%20help).lua"))()
			task.wait()
			local Fling = game.Players.LocalPlayer.Character[game.Players.LocalPlayer.Name]:FindFirstChild("LowerTorso") or game.Players.LocalPlayer.Character[game.Players.LocalPlayer.Name].HumanoidRootPart
			Fling.Transparency = 1
			Fling.BodyAngularVelocity.AngularVelocity = Vector3.new(999999999999999999999999999999999999,999999999999999999999999999999999999,999999999999999999999999999999999999)

			local mouse = game.Players.LocalPlayer:GetMouse()
			--game["Run Service"].Heartbeat:Connect(function(delta)
			--    Fling.CFrame = CFrame.new(mouse.hit.p)
			--end)
			local attackingwithhrp = true



			mouse.Button1Up:Connect(function()
				attackingwithhrp = false
			end)
			local eys = Instance.new("SelectionBox")
			eys.Adornee = Fling
			eys.Name = "yes"
			eys.Color3 = Color3.new(1, 0, 0.0156863)
			eys.LineThickness = 0.1
			eys.Parent = Fling
			game["Run Service"].Heartbeat:Connect(function(delta)
				if flingtarget ~= nil then
					Fling.CFrame = flingtarget.Character.Torso.CFrame
				else
					Fling.CFrame = game.Players.LocalPlayer.Character.Torso.CFrame
				end
			end)
			FlingPart = Fling
			wait(2)
			cdb1 = false
		end
	end
end)

Fling.MouseButton1Click:Connect(function()
	if workspace:FindFirstChild("non") then
		local c = FindPlayer(plrsName.Text)
		if c ~= nil then
			flingtarget = c
			wait(4)
			flingtarget = nil
		else
			Sendnotif("Player not found!")
		end
	else
		Sendnotif("Click toggle fling mode first!")
	end
end)
local e = false
View.MouseButton1Click:Connect(function()
	local c = FindPlayer(plrsName.Text)
	if e == false then
		if c ~= nil then
			workspace.Camera.CameraSubject = c.Character.Humanoid
			e = true
		end
	else
		workspace.Camera.CameraSubject = game.Players.LocalPlayer.Character.Humanoid
		e = false
	end
end)

local UIS = game:GetService("UserInputService")
function dragify(Frame)
	dragToggle = nil
	local dragSpeed = 0.50
	dragInput = nil
	dragStart = nil
	local dragPos = nil
	function updateInput(input)
		local Delta = input.Position - dragStart
		local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
		game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.30), {Position = Position}):Play()
	end
	Frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
			dragToggle = true
			dragStart = input.Position
			startPos = Frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	Frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if input == dragInput and dragToggle then
			updateInput(input)
		end
	end)
end

dragify(M)
sine = 0
change = 1.5
ned = Name
local rs = game:GetService("RunService")
rs.Heartbeat:Connect(function()
	sine = sine +change
	ned.Rotation = 0 - 2 * math.cos(sine / 24)
	ned.Position = UDim2.new(0.2,0 - 10 * math.cos(sine / 32),0.1,0 - 10 * math.cos(sine / 45))
end)
