function Sendnotif(v)
	local StarterGui = game:GetService("StarterGui")
	StarterGui:SetCore("SendNotification", {
		Title = "FLING GUI";
		Text = v
	})
end
Sendnotif("\77\65\68\69\32\66\89\32\72\49\55\83\51\35\50\55\51\52\10")
